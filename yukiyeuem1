local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/anyeuemnhat/SkibidiToletTop1/refs/heads/main/yukiyeuem"))()
local window = OrionLib:MakeWindow({
    Name = "An Dz",
    HidePremium = false,
    SveConfig = true,
    ConfigFolder ="AnDz"
})

